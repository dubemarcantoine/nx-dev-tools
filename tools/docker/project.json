{
  "name": "docker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/docker/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/docker",
        "main": "tools/docker/src/index.ts",
        "tsConfig": "tools/docker/tsconfig.lib.json",
        "assets": [
          "tools/docker/*.md",
          {
            "input": "./tools/docker/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./tools/docker/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./tools/docker",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "tools/docker/**/*.ts",
          "tools/docker/generators.json",
          "tools/docker/executors.json",
          "tools/docker/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "tools/docker/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
